pipeline {

    // Where to run stuff.
    agent any

    // What to run goes here.
    stages {
        stage('Stage_1') {
            steps {
                bat 'echo "Hello i am in stage one"' 
            }
        }
        stage('Stage_02'){
            steps {
                bat 'echo "Hello iam in stage 2"'
            }
        }
    }
}
